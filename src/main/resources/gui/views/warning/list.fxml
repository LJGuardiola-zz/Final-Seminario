<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" styleClass="container"
           xmlns="http://javafx.com/javafx/8.0.171">
    <VBox>
        <TableView fx:id="table" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="spaceNameColumn" text="Espacio" />
                <TableColumn fx:id="spaceTypeColumn" text="Tipo" />
                <TableColumn fx:id="capacityColumn" text="Capacidad" />
                <TableColumn fx:id="registeredColumn" text="Registrados" />
                <TableColumn fx:id="surpassedColumn" text="Superado por" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </VBox>
</StackPane>
