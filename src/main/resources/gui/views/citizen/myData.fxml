<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="205.0" prefWidth="360.0">
        <StackPane rotate="-1.2" styleClass="effect-paper" />
        <StackPane rotate="1.2" styleClass="effect-paper" />
        <VBox prefHeight="196.0" prefWidth="360.0" spacing="30.0" styleClass="container-box">
         <children>
            <VBox spacing="30.0" VBox.vgrow="ALWAYS">
               <children>
                  <HBox>
                     <children>
                        <JFXTextField fx:id="usernameField" disable="true" editable="false" labelFloat="true" prefHeight="25.0" prefWidth="160.0" promptText="Nombre de usuario" />
                        <Separator visible="false" HBox.hgrow="ALWAYS" />
                        <JFXTextField fx:id="emailField" disable="true" editable="false" labelFloat="true" prefHeight="25.0" prefWidth="160.0" promptText="Email" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <JFXTextField fx:id="firstNameField" labelFloat="true" prefHeight="25.0" prefWidth="160.0" promptText="Nombre" />
                        <Separator visible="false" HBox.hgrow="ALWAYS" />
                        <JFXTextField fx:id="lastNameField" labelFloat="true" prefHeight="25.0" prefWidth="160.0" promptText="Apellido" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <JFXTextField fx:id="cuilField" disable="true" editable="false" labelFloat="true" prefHeight="25.0" prefWidth="160.0" promptText="CUIL" />
                        <Separator visible="false" HBox.hgrow="ALWAYS" />
                        <JFXDatePicker fx:id="birthdateField" defaultColor="#4d4d4d" disable="true" overLay="true" prefHeight="25.0" prefWidth="160.0" promptText="Fecha de nacimiento" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <JFXButton fx:id="updateButton" text="Actualizar">
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-green" />
                     </styleClass>
                  </JFXButton>
               </children>
            </HBox>
         </children></VBox>
    </StackPane>
</StackPane>
